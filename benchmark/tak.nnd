#!/bin/sh
:; #-*- mode: nendo; syntax: scheme -*-;;
:; exec /usr/local/bin/nendo $0 $*

;; takeuchi function ( tarai mawashi bench )
(define (tak x y z)
  (if (>= y x)
      y
      (tak (tak (- x 1) y z)
           (tak (- y 1) z x)
           (tak (- z 1) x y))))

(define (main argv)
  (print (tak 10 5 0)))
