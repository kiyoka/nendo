;; FizzBuzz write in Nendo
(define (fizzbuzz max)
  (map
   (lambda (x)
     (let ((x (+ x 1)))
       (cond
        ((= 0 (% x 15))
         "FizzBuzz")
        ((= 0 (% x 3))
         "Fizz")
        ((= 0 (% x 5))
         "Buzz")
        (else
         x))))
   (range max)))

(foreach
 p
 (fizzbuzz 30))
